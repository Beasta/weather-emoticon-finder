{
  "name": "weather-emoticon-finder",
  "version": "1.1.0",
  "description": "maps weather phrases to a simple set of unicode weather emoticons",
  "main": "build/index.js",
  "scripts": {
    "test": "npm run babel && ./node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/_mocha",
    "babel": "mkdir -p build && babel --presets es2015 index.js -o build/index.js",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "keywords": [
    "weather",
    "underground",
    "unicode",
    "emoji",
    "simple",
    "ideogram",
    "emoticon"
  ],
  "author": "Barry",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "should": "^11.2.1",
    "should-http": "^0.1.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beasta/weather-emoticon-finder.git"
  },
  "bugs": {
    "url": "https://github.com/Beasta/weather-emoticon-finder/issues"
  },
  "homepage": "https://github.com/Beasta/weather-emoticon-finder#readme"
}
